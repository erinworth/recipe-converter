{
"manifest_version": 2,
    "name": "Recipe Calculator",
    "description": "A chrome app that will convert recipes to reflect how many servings you would like to prepare",
    "version": "1.1",

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
              "styles.css"
            ],
            "run_at": "document_end"
        }
    ]
}
